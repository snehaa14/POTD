#include<iostream>
using namespace std;


void solve()
{
    int n;
    cin>>n;
    if(n&1)
    {
    while(n--)
    {
    cout<<2<<" ";
    }
    cout<<endl;
    }
    else{
        while(n>2)
        {
            cout<<2<<" ";
            n--;
        }
        cout<<"1 3"<<endl;
    }
}
int main()
{
int t;
cin>>t;
while(t--)
{
    solve();
}

    return 0;
}
